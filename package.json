{
  "name": "copymitter",
  "version": "2.2.0",
  "description": "copy files with emitter",
  "main": "lib/copymitter.js",
  "bin": {
    "copymitter": "bin/copymitter.js"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "6to5:lib": "babel lib -d legacy/lib",
    "6to5:bin": "babel bin -d legacy/bin",
    "legacy:index": "echo \"module.exports = require('./lib/copymitter');\" > legacy/index.js",
    "build": "redrun 6to5:* legacy:*",
    "wisdom": "npm run build",
    "watch:coverage": "npm run watcher -- npm run coverage",
    "watch:test": "npm run watcher -- npm test",
    "watcher": "nodemon -w test -w lib --exec",
    "test": "tape test/*.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "lint:jscs": "jscs --esnext lib test",
    "lint:jshint": "jshint lib test",
    "lint:eslint": "eslint lib test",
    "lint:eslint:bin": "eslint --rule 'no-console:0' bin",
    "lint": "redrun lint:*"
  },
  "dependencies": {
    "debug": "^3.0.0",
    "findit": "^2.0.0",
    "fs-copy-file": "^1.0.0",
    "glob": "^7.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.0.0",
    "jscs": "^3.0.3",
    "jshint": "^2.8.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "redrun": "^6.0.0",
    "rimraf": "^2.4.4",
    "tape": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/node-copymitter.git"
  },
  "keywords": [
    "copy",
    "emitter",
    "file"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/node-copymitter/issues"
  },
  "homepage": "https://github.com/coderaiser/node-copymitter"
}
